
@using MOLUX.Models
@model IEnumerable<web_getAllPartner_Result>

<div class="col-xs-12 bg-white-bo">
    <div class="col-info">
        <div id="logo-slider" class="owl-theme">
            @foreach (var item in Model)
            {
                <div class="item">
                    <a href="@item.Name" title="@item.Name" target="_blank"><img src="/Images/Manufacturer/@item.Logo" style="height: 50px" /></a>
                </div>
            }
        </div>
    </div>
</div>